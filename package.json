{
  "name": "api-acta-e-commerce",
  "version": "1.0.0",
  "description": "NestJS API for Acta E-Commerce",
  "license": "MIT",
  "prisma": {
    "schema": "./prisma/schema"
  },
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=production node -r tsconfig-paths/register dist/src/main.js dotenv -e .env -- nest start",
    "dev": "dotenv -e .env -- nest start --watch",
    "start:debug": "dotenv -e env/local.env -- nest start --debug --watch",
    "start:prod": "dotenv -e env/production.env -- node -r tsconfig-paths/register dist/src/main.js",
    "lint": "eslint \"{src,test}/**/*.ts\" --fix",
    "test": "dotenv -e env/test.env -- jest",
    "test:watch": "dotenv -e env/test.env -- jest --watch",
    "test:cov": "dotenv -e env/test.env -- jest --coverage",
    "test:debug": "dotenv -e env/test.env -- node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "dotenv -e env/test.env -- jest --config ./test/jest-e2e.json",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "postinstall": "npm run prisma:generate",
    "backfill:referrals": "ts-node scripts/backfill-closure-table.ts",
    "backfill:avatar-urls": "ts-node scripts/backfill-avatar-urls.ts",
    "backfill:avatar-urls:advanced": "ts-node scripts/backfill-avatar-urls.ts advanced",
    "backfill:avatar-urls:quick": "ts-node scripts/backfill-avatar-urls.ts quick",
    "migrate:user-config": "ts-node scripts/migrate-user-config.ts",
    "check:migration-status": "ts-node scripts/check-migration-status.ts",
    "test:birthday-notifications": "ts-node scripts/test-birthday-notifications.ts",
    "test:avatar-urls": "ts-node scripts/test-avatar-urls.ts",
    "update:user-status-kyc": "ts-node scripts/update-user-status-to-pending-kyc.ts",
    "update:user-status-kyc:advanced": "ts-node scripts/update-user-status-to-pending-kyc.ts advanced",
    "update:user-status-kyc:quick": "ts-node scripts/quick-update-user-status.ts",
    "update:user-status-active-only": "npm run update:user-status-kyc",
    "test:kyc-system": "ts-node scripts/test-kyc-system.ts",
    "test:kyc-changing": "npm run test:kyc-system",
    "seed:social-feelings-activities": "ts-node scripts/seed-social-feelings-activities.ts",
    "seed:sale-channel": "ts-node scripts/seed-default-sale-channel.ts",
    "migrate:simplify-post-location": "ts-node scripts/migrate-simplify-post-location.ts",
    "undo:order": "ts-node scripts/undo-checkout.ts order",
    "undo:user": "ts-node scripts/undo-checkout.ts user",
    "undo:recent": "ts-node scripts/undo-checkout.ts recent",
    "undo:list": "ts-node scripts/undo-checkout.ts list",
    "test:checkout-fix": "ts-node scripts/test-checkout-fix.ts",
    "undo:simple:list": "ts-node scripts/undo-checkout-simple.ts list",
    "undo:simple:undo": "ts-node scripts/undo-checkout-simple.ts undo",
    "test:order-flow": "ts-node scripts/test-order-flow.ts",
    "test:order-api": "ts-node scripts/test-order-api.ts",
    "test:checkout-complete": "ts-node scripts/test-checkout-complete.ts",
    "reset:message-system": "ts-node scripts/reset-message-system.ts",
    "reset:message-system:preview": "ts-node scripts/reset-message-system.ts preview",
    "reset:message-system:execute": "ts-node scripts/reset-message-system.ts execute",
    "reset:message-system:quick": "ts-node scripts/reset-message-system.ts quick",
    "deactivate:products-no-inventory": "ts-node scripts/deactivate-products-without-inventory.ts",
    "debug:inventory-cart": "ts-node scripts/debug-inventory-cart-issue.ts",
    "test:inventory-fixes": "ts-node scripts/test-inventory-fixes.ts",
    "monitor:connections": "node scripts/monitor-connections.js"
  },
  "lint-staged": {
    "*.{js,ts}": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "{*.json,.{prettierrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "dependencies": {
    "@mux/mux-node": "^11.1.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.3",
    "@nestjs/config": "4.0.2",
    "@nestjs/core": "^11.1.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.3",
    "@nestjs/platform-socket.io": "^11.1.3",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.1.3",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/websockets": "^11.1.3",
    "@prisma/client": "6.9.0",
    "@react-email/components": "^0.1.0",
    "@sentry/profiling-node": "^9.34.0",
    "@types/multer": "^1.4.13",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "cache-manager": "^7.0.0",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudinary": "^2.6.1",
    "commander": "^14.0.0",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "multer": "^2.0.1",
    "nanoid": "^3",
    "nodemailer": "^7.0.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "reflect-metadata": "^0.2.2",
    "release-it": "^19.0.3",
    "request-ip": "^3.3.0",
    "resend": "^4.5.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.4.0",
    "socket.io": "^4.8.1",
    "streamifier": "^0.1.1",
    "swagger-ui-express": "^5.0.1",
    "uploadthing": "^7.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/testing": "^11.1.3",
    "@sentry/nestjs": "^9.34.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.14",
    "@types/nanoid": "^3.0.0",
    "@types/node": "^22.15.30",
    "@types/nodemailer": "^6.4.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.33.1",
    "@typescript-eslint/parser": "^8.33.1",
    "cross-env": "^7.0.2",
    "eslint": "^9.28.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^29.7.0",
    "jest-extended": "6.0.0",
    "jest-mock-extended": "4.0.0-beta1",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "prisma": "6.9.0",
    "source-map-support": "^0.5.20",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.3.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "src",
      "test"
    ],
    "setupFilesAfterEnv": [
      "jest-extended",
      "./jest.setup.ts"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
